{
  "name": "ai-article-generator",
  "version": "1.0.0",
  "description": "AI-powered automated article generation pipeline",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "echo 'Build complete - Node.js app ready'",
    "docker:build": "docker build -t ai-article-generator .",
    "docker:run": "docker run -p 3000:3000 --env-file .env ai-article-generator",
    "docker:compose": "docker-compose up --build"
  },
  "keywords": [
    "ai",
    "article",
    "generator",
    "openai",
    "seo",
    "automation",
    "content",
    "writing"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
